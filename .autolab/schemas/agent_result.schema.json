{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "AgentResult",
  "type": "object",
  "required": ["status", "summary", "changed_files", "completion_token_seen"],
  "properties": {
    "status": {
      "type": "string",
      "enum": ["complete", "needs_retry", "failed"]
    },
    "summary": {"type": "string"},
    "changed_files": {
      "type": "array",
      "items": {"type": "string"}
    },
    "completion_token_seen": {"type": "boolean"}
  },
  "additionalProperties": true
}
